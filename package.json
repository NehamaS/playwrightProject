  "scripts": {
    "test:dev:debug": "set PWDEBUG=1 && npx playwright test -g @dev",
    "test:prod:debug": "set PWDEBUG=1 && npx playwright test -g @prod",
    "test:main:debug": "set PWDEBUG=1 && npx playwright test -g @main",
    "test:dev:headless": "npx playwright test -g @dev",
    "test:prod:headless": "npx playwright test -g @prod",
    "test:main:headless": "npx playwright test -g @main",
    "test:dev:headed": "npx playwright test -g @dev --headed",
    "test:prod:headed": "npx playwright test -g @prod --headed",
    "test:main:headed": "npx playwright test -g @main --headed",
    "build": "tsc --build",
    "clean": "tsc --build --clean"
  },
